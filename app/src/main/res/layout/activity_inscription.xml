<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InscriptionActivity">

    <EditText
        android:id="@+id/editTextTextInscriptionSite"
        android:layout_width="162dp"
        android:layout_height="31dp"
        android:layout_marginStart="23dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="13dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textViewInscriptionExposition"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionSite" />

    <EditText
        android:id="@+id/editTextTextInscriptionMail"
        android:layout_width="162dp"
        android:layout_height="31dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="154dp"
        android:layout_marginBottom="14dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionSite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionEmail" />

    <EditText
        android:id="@+id/editTextTextInscriptionTelephone"
        android:layout_width="162dp"
        android:layout_height="31dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="7dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionMail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionTelephone" />

    <EditText
        android:id="@+id/editTextTextInscriptionActiviteEntreprise"
        android:layout_width="165dp"
        android:layout_height="34dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="25dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionNom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionActiviteEntreprise" />

    <TextView
        android:id="@+id/textViewInscriptionSecteurSalon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Secteur du salon souhaité :"
        app:layout_constraintTop_toTopOf="@+id/guidelineSecteurTop"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintBottom_toBottomOf="@+id/guidelineSpinnerBottom"
        app:layout_constraintStart_toStartOf="@id/guideline_verticalWidget" />

    <TextView
        android:id="@+id/textViewInscriptionExposition"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Exposez-vous pour la 1ère fois sur le salon ?"
        app:layout_constraintBottom_toBottomOf="@+id/guidelineSecteurTop"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintStart_toStartOf="@id/guideline_verticalWidget"
        app:layout_constraintTop_toTopOf="@+id/guidelineExpositionTop" />

    <TextView
        android:id="@+id/textViewInscriptionSite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="12dp"
        android:text="Site internet"
        app:layout_constraintBottom_toTopOf="@+id/textViewInscriptionExposition"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionSite"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewInscriptionEmail"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="12dp"
        android:text="E-mail"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionMail"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="311dp" />

    <TextView
        android:id="@+id/textViewInscriptionTelephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="7dp"
        android:text="Telephone"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionMail"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionTelephone"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewInscriptionPrenom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="48dp"
        android:text="Prenom"
        app:layout_constraintBottom_toTopOf="@+id/textViewInscriptionTelephone"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionPrenom"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewInscriptionNom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="27dp"
        android:text="Nom"
        app:layout_constraintBottom_toTopOf="@+id/textViewInscriptionPrenom"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionNom" />

    <TextView
        android:id="@+id/textViewInscriptionActiviteEntreprise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="26dp"
        android:text="Activité de l'entreprise :"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionNom"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionActiviteEntreprise"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewInscriptionRaison"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="25dp"
        android:text="Raison sociale :"
        app:layout_constraintBottom_toTopOf="@+id/textViewInscriptionActiviteEntreprise"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextInscriptionRaisonSociale"
        app:layout_constraintStart_toStartOf="parent" />


    <Switch
        android:id="@+id/switchInscriptionExposition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="322dp"
        tools:layout_editor_absoluteY="388dp" />

    <EditText
        android:id="@+id/editTextTextInscriptionRaisonSociale"
        android:layout_width="174dp"
        android:layout_height="34dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionActiviteEntreprise"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionRaison" />

    <EditText
        android:id="@+id/editTextTextInscriptionNom"
        android:layout_width="162dp"
        android:layout_height="31dp"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionPrenom"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionNom" />

    <EditText
        android:id="@+id/editTextTextInscriptionPrenom"
        android:layout_width="182dp"
        android:layout_height="35dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextInscriptionTelephone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewInscriptionPrenom" />

    <Spinner
        android:id="@+id/spinnerSecteur"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/guidelineSpinnerTop"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintBottom_toBottomOf="@+id/guidelineSpinnerBottom"
        app:layout_constraintStart_toStartOf="@id/guideline_verticalWidget" />

    <Button
        android:id="@+id/buttonInscriptionQuitter"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Quitter"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/guideline6"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintTop_toTopOf="@id/guideline3" />

    <Button
        android:id="@+id/buttonInscriptionExposant"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="S'inscrire"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/guideline6"
        app:layout_constraintEnd_toEndOf="@id/guideline2"
        app:layout_constraintStart_toStartOf="@id/guideline_verticalWidget"
        app:layout_constraintTop_toTopOf="@id/guideline3" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_verticalWidget"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.10" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.45" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.90" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.92" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineSpinnerTop"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineSpinnerBottom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.73" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineSecteurTop"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineExpositionTop"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.53" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineSiteTop"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.47" />




</androidx.constraintlayout.widget.ConstraintLayout>